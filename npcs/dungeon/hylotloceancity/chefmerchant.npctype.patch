[{"op": "replace", "path": "/scriptConfig/dialog/merchant/start/novakid/default/0", "value": "Du verlangst deinen Bauch zu füllen?"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/novakid/default/1", "value": "Ich serviere das beste Essen auf dieser Seite des Sonnensystems!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/novakid/default/2", "value": "Meine Gerichte sind astrein!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/novakid/default/3", "value": "Mein galaktisch gutes Essen wird dich zu den Sternen schicken!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/novakid/default/1", "value": "Ich sehe du bist ein Feinschmecker."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/novakid/default/2", "value": "Ich hoffe, dir schmeckt das Essen."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/human/default/1", "value": "Was kann ich dir bringen? Guck dir mein Angebot an."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/novakid/default/0", "value": "Ich freue mich auf deinen nächsten Einkauf."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/hylotl/default/1", "value": "Meine Frischwaren sind mehr, als nur schmackhaft - sie sind eine Offenbarung!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/floran/default/1", "value": "Floraner wird dich wiedersehen!"}, {"op": "replace", "path": "/scriptConfig/merchant/paneLayoutOverride/windowtitle/title", "value": "Koch"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/avian/default/0", "value": "Bis bald!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/avian/default/1", "value": "Immer wieder eine Freude. Genieß dein Essen!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/avian/default/2", "value": "Komm wieder, hörst du?"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/floran/default/2", "value": "Floraner kann esss nicht erwarten, mehr Essssen zzzu verkaufen!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/glitch/default/0", "value": "Fröhlich. Komm jederzeit wieder."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/glitch/default/1", "value": "Ermutigend. Mach weiter und werde hungrig."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/glitch/default/2", "value": "Erfreut. Ich bin sicher, dass du es genießen wirst."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/hylotl/default/0", "value": "Ich freue mich auf zukünftige Geschäfte."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/floran/default/4", "value": "Floraner steht zzzu Diensssten."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/hylotl/default/1", "value": "Ich kann dir ansehen, dass du ein Feinschmecker bist."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/apex/default/0", "value": "Komm wieder, wann immer du hungrig bist!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/apex/default/1", "value": "Sei kein Fremder."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/apex/default/2", "value": "Frischwaren jeden Tag!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/floran/default/0", "value": "Komm bald für mehr Essssen zzzurück!"}, {"op": "replace", "path": "/scriptConfig/merchant/paneLayoutOverride/windowtitle/subtitle", "value": "Verkauft leckeres Essen!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/avian/default/1", "value": "Womit kann ich dienen?"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/human/default/2", "value": "Frische Vorräte nötig? Nicht weitersuchen!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/human/default/3", "value": "Du wirst kein besseres Essen finden! Jedenfalls nicht hier in der Umgebung."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/human/default/4", "value": "Diese Made ist wirklich köstlich."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/default/default/0", "value": "Hungrig? Ich habe viele Frischwaren gleich hier!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/hylotl/default/3", "value": "Alles, was ich verkaufe ist mit Liebe gewachsen. Und Kompost."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/hylotl/default/4", "value": "Komm, probier meine göttliche Kost!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/human/default/0", "value": "Womit kann ich dein Interesse wecken?"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/human/default/2", "value": "Immer gerne zu Diensten!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/floran/default/0", "value": "Floraner verkauft Esssen!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/glitch/default/0", "value": "Umgänglich. Alles, was du zum Kochen brauchst, gleich hier."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/glitch/default/3", "value": "Herzlich. Beste Waren im Umkreis! Was kann ich dir besorgen?"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/apex/default/4", "value": "Ich hoffe, du bist hungrig!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/avian/default/2", "value": "Bitte, versuch meine Frischwaren. Ich weiss, dass du sie lieben wirst."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/human/default/1", "value": "Komm wieder, wann immer du hungrig bist!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/glitch/default/2", "value": "Freundlich. Wenn du hungrig bist, bist du bei mir richtig."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/default/default/0", "value": "Komm jederzeit wieder!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/hylotl/default/2", "value": "Ich hoffe, du lässt es dir schmecken."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/end/human/default/0", "value": "Genieße es, du wirst nichts Besseres finden!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/floran/default/2", "value": "Greenfinger sagt, Floraner issst \"ungeschickt in der Küche\", aber guter Warenhändler."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/floran/default/3", "value": "Du siehssst aus, wie schlechter Jäger, also Floraner verkauft dir Mahlzzzeit."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/apex/default/0", "value": "Wenn du was zu Essen willst, bekommst du es hier."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/apex/default/1", "value": "Meine Vorräte kommen von sicheren Quellen."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/floran/default/1", "value": "Floraner's Essssen issst das bessste! Kauf allesss!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/avian/default/3", "value": "Kann ich dich für einige sehr feine Vorräte begeistern?"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/avian/default/4", "value": "Frischwaren für alle deine Kochbedürfnisse!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/avian/default/0", "value": "Ich bin mir sicher, dass du mein Essen lieben wirst."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/apex/default/2", "value": "Meine Frischwaren sind köstlich, du findest keine Besseren."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/apex/default/3", "value": "Ich hoffe du magst Bananen."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/glitch/default/4", "value": "Stolz. Glitch bauen die besten Waren an."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/glitch/default/1", "value": "Zuversichtlich. Meine Waren sind von höchster Qualität!"}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/hylotl/default/0", "value": "Meine Frischwaren sind einfach köstlich."}, {"op": "replace", "path": "/scriptConfig/dialog/merchant/start/hylotl/default/2", "value": "Ich bezweifle, dass du köstlicheres Essen finden kannst, als das, was ich anbiete."}]