[{"op": "replace", "path": "/scriptConfig/dialog/tenant/grumbles/final/default/default/1", "value": "ICH KANN UNTER DIESEN UMSTÄNDEN NICHT WEITER HIER LEBEN. ZEIT, WEITERZUZIEHEN, IN DAS FLEISCHIGE UNGEWISSE!"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/grumbles/tagCriteria/default/default/1", "value": "WAS IST MIT MEINEN FLEISCHIGEN MÖBELN PASSIERT?"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/0", "value": "ICH BIN EIN HÄNDLER VON SELTENEN FLEISCHIGEN WAREN! ICH WERDE VERSUCHEN, ETWAS NETTES FÜR DICH ZU FINDEN!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/1", "value": "ICH SUCHE NACH SELTENEN FLEISCHIGEN SCHÄTZEN! KOMM MICH IRGENDWANN BESUCHEN, ICH KÖNNTE DANN EIN GESCHENK FÜR DICH HABEN!"}, {"op": "replace", "path": "/scriptConfig/dialog/converse/default/default/2", "value": "ICH DURCHSUCHE ALLES NACH FLEISCHIGEN SCHÄTZEN! ICH WERDE DIR ERZÄHLEN, WENN ICH FÜR DICH ETWAS GEFUNDEN HABE!"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/arrivedHome/rent/default/default/0", "value": "ICH KAM DURCH HANDELN DARAN! ES IST EIN GESCHENK FÜR DICH!"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/arrivedHome/rent/default/default/1", "value": "AUF MEINEN HANDELSREISEN BIN ICH FÜR DICH DARÜBER GESTOLPERT! ICH HOFFE, ES GEFÄLLT DIR!"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/arrivedHome/beacon/default/default/0", "value": "HEY, BRAUCHST DU MICH FÜR IRGENDWAS?"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/arrivedHome/beacon/default/default/1", "value": "HE DA, ALLES FIT?"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/grumbles/final/default/default/0", "value": "NUN DENN, ZEIT, MEIN FLEISCHIGES GLÜCK ANDERSWO ZU SUCHEN!"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/grumbles/otherDeed/default/default/0", "value": "HEY, NIEMAND SONST DARF HIER EINZIEHEN, KLAR?"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/grumbles/otherDeed/default/default/1", "value": "WAR DER ORT NICHT FÜR MICH BESTIMMT?"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/grumbles/severe/default/default/0", "value": "ICH KANN EINFACH NICHT NACH FLEISCHIGEN WAREN SCHAUEN WENN MEIN HEIM SO IST!"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/grumbles/severe/default/default/1", "value": "WIE SOLL ICH UNTER DIESEN BEDINGUNGEN NACH FLEISCHIGEN WAREN SCHAUEN?"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/grumbles/enclosedArea/default/default/0", "value": "ICH GLAUBE, DIE WAND IST HIER KAPUTT! KANNST DU HELFEN?"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/grumbles/enclosedArea/default/default/1", "value": "IST EIN WENIG ZUGIG HIER, HABE ICH IRGENDWO EINE KAPUTTE WAND?"}, {"op": "replace", "path": "/scriptConfig/dialog/tenant/grumbles/tagCriteria/default/default/0", "value": "ES WAR VORHER MEHR FLEISCHIGES ZEUG HIER DRIN ALS JETZT!"}]